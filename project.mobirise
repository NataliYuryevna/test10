          "_customHTML": "<section class=\"testimonials2 slider slider-with-bullets\" group=\"Testimonials\" plugins=\"amp-main, amp-carousel, amp-bind\" custom-thumb=\"assets/thumb/testimonials.jpg\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header mbr-if=\"showTitle\">Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" required=\"\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" required=\"\">\n        <input type=\"range\" inline=\"\" title=\"Padding Slide\" name=\"slideWidth\" min=\"0\" max=\"5\" step=\"1\" value=\"3\" condition=\"typeSlide == 'slides'\">\n        <input type=\"checkbox\" title=\"Row/Column\" name=\"flexDirection\" checked=\"\">\n\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n\n    <header>Cards</header>\n        <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"typeSlide == 'carousel'\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showCardTitle || showCardText\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Name\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Title\" name=\"lineTitle\" condition=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showCardText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\">\n        <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#cccccc\" condition=\"showBorders\">\n        <input type=\"checkbox\" title=\"Transparent\" name=\"transparentBg\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#f4f5f7\" condition=\"!transparentBg\">\n\n    <header>Image</header>\n        <input type=\"checkbox\" title=\"Rounded\" name=\"roundedImage\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Width\" name=\"imageWidth\" min=\"10\" max=\"100\" step=\"1\" value=\"18\" condition=\"flexDirection\">\n        <input type=\"range\" inline=\"\" title=\"Width\" name=\"widthImg\" min=\"1\" max=\"20\" step=\"1\" value=\"6\" condition=\"!flexDirection\">\n\n    <header>Sliders</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" checked=\"\" condition=\"slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n            <input type=\"range\" inline=\"\" title=\"Delay\" name=\"delay\" min=\"1000\" max=\"5000\" step=\"1000\" value=\"3000\" condition=\"autoplay &amp;&amp; slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n        <input type=\"range\" inline=\"\" title=\"Slider Height\" name=\"slideHeight\" min=\"250\" max=\"700\" step=\"10\" value=\"440\">\n        <select title=\"Type Slide\" name=\"typeSlide\">\n            <option value=\"slides\">Slides</option>\n            <option value=\"carousel\" selected=\"\">Carousel</option>\n        </select>\n        <select title=\"Sliders Count\" name=\"slidersAmount\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=