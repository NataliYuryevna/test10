        {
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card, .card-img, .card-box": {
              "width": "100%"
            },
            ".card-wrapper when not (@transparentBg), (@showBorders)": {
              "@media (min-width: 992px)": {
                "padding": "40px 30px"
              }
            },
            ".card-wrapper when not (@transparentBg)": {
              "background-color": "@cardBg"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-img amp-img when (@flexDirection)": {
              "width": "~\"@{imageWidth}%\"",
              "max-width": "370px",
              "min-width": "60px"
            },
            ".card-wrapper when not (@flexDirection)": {
              "flex-direction": "row",
              "amp-img, .card-img": {
                "width": "~\"@{widthImg}rem\"",
                "height": "auto",
                "@media (max-width: 767px)": {
                  "width": "70px"
                }
              },
              ".card-box": {
                "@media (min-width: 992px)": {
                  "padding-left": "30px"
                }
              }
            },
            ".card-subtitle:before when (@lineSpecialty) and (@showCardSubtitle)": {
              "content": "''",
              "width": "25px",
              "height": "3px",
              "display": "inline-block",
              "vertical-align": "middle",
              "margin-right": "10px",
              "background-color": "@primaryColor"
            },
            ".icons-list .amp-iconfont when (@showSocial)": {
              "font-size": "~\"calc(@{iconsSize}rem * 0.42)\"",
              "line-height": "~\"calc(@{iconsSize}rem - 4px)\"",
              "width": "~\"@{iconsSize}rem\"",
              "height": "~\"@{iconsSize}rem\"",
              "margin-bottom": "10px"
            },
            "amp-img.mbr-ml-auto when (@flexDirection) and (@cardAlign = '3')": {
              "@media (max-width: 767px)": {
                "margin-right": "auto"
              }
            },
            ".mbr-row when not (@showPadding)": {
              ".card": {
                "padding": "0"
              },
              "& when (@showBorders)": {
                "& when (@showTopBorder)": {
                  "border-top": "1px solid @bordersColor"
                },
                ".card-wrapper": {
                  "border": "none"
                },
