          "_name": "team1",
          "_customHTML": "<section class=\"teams2\" group=\"Teams\" plugins=\"amp-main\" custom-thumb=\"assets/thumb/team.jpg\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header mbr-if=\"showTitle\">Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" required=\"\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" required=\"\">\n        <input type=\"checkbox\" title=\"Row/Column\" name=\"flexDirection\" checked=\"\">\n\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n\n    <header>Cards</header>\n        <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showCardTitle || showCardSubtitle || showCardText || showExperience || showEmail || showTel || showSocial\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Name\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Specialty\" name=\"showCardSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Specialty\" name=\"lineSpecialty\" condition=\"showCardSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Experience\" name=\"showExperience\">\n        <input type=\"checkbox\" title=\"Show Email\" name=\"showEmail\">\n        <input type=\"checkbox\" title=\"Show Telephone\" name=\"showTel\">\n        <input type=\"checkbox\" title=\"Show Social\" name=\"showSocial\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Count Social\" name=\"socialCount\" min=\"1\" max=\"6\" step=\"1\" value=\"3\" condition=\"showSocial\">\n        <input type=\"range\" inline=\"\" title=\"Size Social\" name=\"iconsSize\" min=\"0.8\" max=\"4\" step=\"0.1\" value=\"2.7\" condition=\"showSocial\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showCardText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Padding\" name=\"showPadding\" checked=\"\">\n        <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\">\n        <input type=\"checkbox\" title=\"Top Border\" name=\"showTopBorder\" condition=\"showBorders &amp;&amp; !showPadding\">\n        <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#615779\" condition=\"showBorders\">\n        <input type=\"checkbox\" title=\"Transparent\" name=\"transparentBg\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#f4f5f7\" condition=\"!transparentBg\">\n\n    <header>Image</header>\n        <input type=\"checkbox\" title=\"Rounded\" name=\"roundedImage\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Width\" name=\"imageWidth\" min=\"10\" max=\"100\" step=\"1\" value=\"45\" condition=\"flexDirection\">\n        <input type=\"range\" inline=\"\" title=\"Width\" name=\"widthImg\" min=\"1\" max=\"10\" step=\"0.1\" value=\"5\" condition=\"!flexDirection\">\n\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\" required=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/ca02.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#fffcfd\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f2eeef\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#f2eeef\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left': titleAlign==1,\n                         'align-center': titleAlign==2,\n                         'align-right': titleAlign==3}\">\n            <h3 class=\"mbr-section-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">Our Team</h3>\n            <h4 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">\n                Team Subtitle</h4>\n        </div>\n        <div class=\"mbr-row mbr-jc-c\" mbr-class=\"{'mbr-px-2': !fullWidth &amp;&amp; !showPadding}\">\n            <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6':(cardsAmount < 3 &amp;&amp; flexDirection) || !flexDirection, 'mbr-col-md-4':cardsAmount == 3 &amp;&amp; flexDirection, 'mbr-col-md-6 mbr-col-lg-3 md-pb':cardsAmount == 4 &amp;&amp; flexDirection, 'last-child': cardsAmount == 1,'lg-pb md-pb': !flexDirection &amp;&amp; cardsAmount > 2 }\">\n                <div class=\"card-wrapper\" mbr-class=\"{'align-center': cardAlign==2,\n                         'align-right': cardAlign==3, 'mbr-p-2': !transparentBg || showBorders, 'mbr-flex': !flexDirection}\">\n                    <div class=\"card-img\">\n                        <amp-img class=\"placeholder-loader\" mbr-class=\"{'rounded': roundedImage, 'mbr-m-auto': cardAlign==2 &amp;&amp; flexDirection, 'mbr-ml-auto': cardAlign==3 &amp;&amp; flexDirection}\" src=\"@PROJECT_PATH@/assets/images/ca15.jpg\" layout=\"responsive\" width=\"157\" height=\"157\" alt=\"image\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/ca15.jpg\" alt=\"\" title=\"\">\n                        </amp-img>\n                    </div>\n                    <div class=\"card-box\" mbr-if=\"showCardTitle || showCardSubtitle || showCardText || showExperience || showEmail || showTel || showSocial\" mbr-class=\"{'mbr-px-2': (fullWidth &amp;&amp; !showPadding &amp;&amp; !showBorders &am